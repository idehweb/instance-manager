{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "ignorePatterns":["node_modules","dist"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-empty":"warn",
        "no-unused-vars":"warn"
    }
}
